<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/drawerlayout_ad"

    >
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.sweekritee.notifyme.NavigationBarAdmin"
        android:id="@+id/navbar_std"
        android:orientation="vertical">

    </LinearLayout>
    <android.support.design.widget.NavigationView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/nav_ad"
            app:headerLayout="@layout/nav_header_ad"
            app:menu="@menu/navigation_menu_ad"
            android:layout_gravity="start"
           >

        </android.support.design.widget.NavigationView>






</android.support.v4.widget.DrawerLayout>
